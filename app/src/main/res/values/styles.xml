<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomCheckbox" parent="@style/Widget.AppCompat.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_selector</item>
    </style>

    <style name="CustomTextButton_g" parent="@style/Widget.AppCompat.TextView">
        <item name="android:layout_width">54dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:textSize">11dp</item>
        <item name="android:textColor">#7e7e7e</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/roundbtn_g4</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="CustomTextButton_y" parent="@style/Widget.AppCompat.TextView">
        <item name="android:layout_width">54dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:textSize">11dp</item>
        <item name="android:textColor">#7e7e7e</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/roundbtn_y4</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="Picker" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textColorPrimary">#7e7e7e</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textStyle">bold</item>
        <item name="colorControlNormal">#00000000</item>
    </style>

    <style name="CustomCalendar" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">@color/black</item>
        <item name="android:weekNumberColor">@android:color/holo_red_light</item>
        <item name="android:weekSeparatorLineColor">@color/black</item>
    </style>


</resources>